{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
    "dev": "concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css",
    "typecheck": "tsc -b",
    "db:reset": "npx supabase db reset",
    "db:dump": "npx supabase db dump -f supabase/seed.sql",
    "db:gentypes": "npx supabase gen types typescript --project-id uauexnudsfebogdqdzfe > db_types.ts",
    "test": "jest --watchAll",
    "test:email1": "./dbseed/curl.sh ./dbseed/email1.json",
    "test:email2": "./dbseed/curl.sh ./dbseed/email2.json",
    "test:email3": "./dbseed/curl.sh ./dbseed/email3.json"
  },
